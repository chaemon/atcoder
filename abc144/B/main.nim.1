let YES = "Yes"
let NO = "No"

proc solve(N:int) =
  for i in 1..9:
    if N mod i == 0:
      let j = N div i
      if j <= 9:
        echo YES
        return
  echo NO
  return

#{{{ main function
proc main() =
  var N = 0
  N = nextInt()
  solve(N);
  return

